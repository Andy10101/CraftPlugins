<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#00cc9d" android:endColor="#00cc9d" android:type="1" android:gradientRadius="50.0" />
            <stroke android:width="1dp" android:color="#ffffff" />
            <corners android:radius="3dp" />
            <padding android:left="2dp" android:top="1dp" android:right="2dp" android:bottom="1dp" />
        </shape>
    </item>
</selector>
