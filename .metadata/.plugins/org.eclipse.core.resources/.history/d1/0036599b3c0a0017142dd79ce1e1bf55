package pc;

public class PC {
	String name;
	String hash = null;
	
	public PC(String hash){
		this.name = "unamed";
		this.hash = hash;
	}
	
	public PC(String name, String hash){
		this.name =  name;
		this.hash = hash;
	}
	@Override
	public String toString(){
		return name+"|"+hash;
	}
	@Override
	public boolean equals(Object obj){
		if(obj instanceof PC){
			if()
		}
		return false;
	}

}
